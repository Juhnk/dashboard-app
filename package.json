{
  "name": "mustache-cashstache",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "node scripts/wsl2-port-manager.js dev",
    "dev:web": "turbo dev --filter=@mustache/web",
    "dev:simple": "node scripts/dev-setup.js",
    "build": "turbo build",
    "test": "turbo test",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest --coverage",
    "lint": "turbo lint",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo typecheck",
    "storybook": "cd apps/web && npm run storybook",
    "build-storybook": "cd apps/web && npm run build-storybook",
    "chromatic": "cd apps/web && npm run chromatic",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:restart": "npm run docker:down && npm run docker:dev",
    "db:setup": "npm run docker:dev && sleep 5",
    "clean": "turbo clean && rm -rf node_modules",
    "reset": "npm run clean && npm install",
    "health": "node scripts/health-check.js",
    "wsl2:diagnose": "node scripts/wsl2-diagnostics.js",
    "wsl2:optimize": "node scripts/wsl2-optimizer.js",
    "wsl2:ports": "node scripts/wsl2-port-manager.js",
    "wsl2:setup": "./scripts/setup-wsl2-dev.sh",
    "ports": "node scripts/wsl2-port-manager.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@playwright/test": "^1.40.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.0.0",
    "chromatic": "^13.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "semantic-release": "^22.0.8",
    "storybook": "^8.6.14",
    "turbo": "^2.0.6",
    "typescript": "^5.5.2",
    "vitest": "^1.0.0"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.14"
  }
}
